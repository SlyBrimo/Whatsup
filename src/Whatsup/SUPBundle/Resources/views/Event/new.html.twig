{% extends '::base.html.twig' %}
{% block body %}

<form action="{{ path('event_create') }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
    <p>
        <button type="submit">Create</button>
    </p>
</form>

<ul class="record_actions">
    <li>
        <a href="{{ path('event') }}">
            Back to the list
        </a>
    </li>
</ul>
<script>
$(function() {
    $( "#event_startdate" ).datepicker();
    $( "#event_enddate" ).datepicker();
});
</script>
{% endblock %}
